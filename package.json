{
  "name": "boilerplate-webpack",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:development": "export NODE_ENV=development && yarn create-app-config-file && webpack-dev-server --config ./webpack/configuration/development.configuration.babel.js",
    "build:production": "export NODE_ENV=production && yarn create-app-config-file && webpack --config ./webpack/configuration/production.configuration.babel.js",
    "serve-production": "live-server --entry-file=dist/index.html --port=5000",
    "create-app-config-file": "babel-node scripts/create-app-config-file.js",
    "test": "echo 'test'"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "live-server": "^1.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
